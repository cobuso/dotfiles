# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi




source $HOME/.zsh_aliases

export PATH="$HOME/Tools/git-deploy/bin:$HOME/Tools/sesame.sh:$HOME/Tools/reattach-to-user-namespace/bin:/usr/local/bin:/usr/local/sbin:$PATH"
export EDITOR=vim

setopt extended_glob

# Tmuxinator
[[ -s $HOME/.tmuxinator/scripts/tmuxinator ]] && source $HOME/.tmuxinator/scripts/tmuxinator

# Fast git completion
__git_files () { 
    _wanted files expl 'local files' _files     
}

# virtualenvwrapper
if [ -f /usr/local/bin/virtualenvwrapper.sh ]; then
  source /usr/local/bin/virtualenvwrapper.sh
fi
if [ ! -d ~/Envs ]; then
  mkdir ~/Envs
fi
export WORKON_HOME=~/Envs

#Â tmuxinator completion
source ~/Dotfiles/zsh/completion/tmuxinator.zsh
export DISABLE_AUTO_TITLE=true

# RVM
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"  # This loads RVM into a shell session.
export PATH="$PATH:$HOME/.rvm/bin" # Add RVM to PATH for scripting
